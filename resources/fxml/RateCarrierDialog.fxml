<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox spacing="12.0" alignment="CENTER" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.RateCarrierDialogController" style="-fx-padding:20; -fx-background-color: white;">
    <children>
        <Label fx:id="titleLabel" text="Rate Carrier" />
        <HBox spacing="8" alignment="CENTER">
            <children>
                <Button fx:id="star1" text="☆" onAction="#onStar1" />
                <Button fx:id="star2" text="☆" onAction="#onStar2" />
                <Button fx:id="star3" text="☆" onAction="#onStar3" />
                <Button fx:id="star4" text="☆" onAction="#onStar4" />
                <Button fx:id="star5" text="☆" onAction="#onStar5" />
            </children>
        </HBox>
        <HBox spacing="10" alignment="CENTER_RIGHT">
            <children>
                <Button fx:id="cancelBtn" text="Cancel" onAction="#onCancel" />
                <Button fx:id="okBtn" text="OK" onAction="#onOk" />
            </children>
        </HBox>
    </children>
</VBox>
